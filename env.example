# Copy this file to a new file named .env (same folder) and fill in your real key.
# IMPORTANT: Do NOT expose keys in the browser. This app uses a local API server.

# OpenRouter API (Recommended - Free tier with Grok 4.1 Fast)
OPENROUTER_API_KEY=sk-or-v1-your_key_here
# Grok 4.1 Fast model (supports vision/image analysis)
OPENROUTER_MODEL=x-ai/grok-4.1-fast

# OpenAI API (Alternative - if you prefer)
OPENAI_API_KEY=sk-your_key_here
OPENAI_MODEL=gpt-4o-mini

PORT=8787
# AI Provider: 'openrouter' or 'openai'
AI_PROVIDER=openrouter
